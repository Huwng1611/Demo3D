using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Sound : MonoBehaviour
{
	public static Sound instance;
	public AudioSource sound;

	void Awake ()
	{
		instance = this;
		DontDestroyOnLoad (this);
	}

	void Start ()
	{
		sound = GetComponent<AudioSource> ();
	}
}
